{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
     "plugins": [{ "name": "next" }],
     "esModuleInterop": true,
     "jsx": "preserve",
     "allowJs": true,
     "forceConsistentCasingInFileNames": true,
     "incremental": true

  },
  "include": ["vite.config.ts","./dist/types/**/*.ts","./next-env.d.ts"],
  "exclude": ["./node_modules"]
}
